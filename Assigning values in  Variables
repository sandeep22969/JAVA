class Hello
{
    public static void main(String a[])
    {
        int num1=10;
        int num2=5;
        int add = num1+num2;
        int sub = num1-num2;
        int mul = num1*num2;
        int div = num1/num2;
        System.out.println(add);
        System.out.println(sub);
        System.out.println(mul);
        System.out.println(div);
    }
}
